{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "dotenv -e ../../.env -- nest start --watch",
    "build": "nest build",
    "clean": "rm -rf dist",
    "start": "nest start",
    "lint": "eslint",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:unit": "NODE_ENV=test dotenv -e ../../.env -- jest --config ./test/jest-unit.json",
    "test:e2e": "NODE_ENV=test dotenv -e ../../.env -- jest --config ./test/jest-e2e.json",
    "test:e2e:full": "pnpm test:db-up && pnpm test:db-migrate && pnpm test:e2e; pnpm test:db-down",
    "test:cov": "NODE_ENV=test dotenv -e ../../.env -- jest --config ./test/jest-unit.json --coverage",
    "test:db-up": "dotenv -e ../../.env -- docker compose -f ../../.docker/docker-compose.test.yml -p ar-002-clubs-test up -d",
    "test:db-down": "dotenv -e ../../.env -- docker compose -p ar-002-clubs-test down",
    "test:db-clean": "dotenv -e ../../.env -- docker compose -p ar-002-clubs-test down -v",
    "test:db-migrate": "pnpm test:db-up --wait && NODE_ENV=test dotenv -e ../../.env -- pnpm drizzle-kit push:mysql --config ./src/drizzle/drizzle.config.ts",
    "db-up": "dotenv -e ../../.env -- docker compose -f ../../.docker/docker-compose.dev.yml -p ar-002-clubs up -d",
    "db-down": "dotenv -e ../../.env -- docker compose -p ar-002-clubs down",
    "db-generate": "dotenv -e ../../.env -- pnpm db-up --wait && dotenv -e ../../.env -- pnpm drizzle-kit push:mysql --config ./src/drizzle/drizzle.config.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.609.0",
    "@aws-sdk/s3-request-presigner": "^3.609.0",
    "@clubs/domain": "workspace:*",
    "@clubs/interface": "workspace:*",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.10",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/websockets": "^10.3.10",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^10.5.0",
    "@types/jsdom": "^21.1.7",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.7",
    "cookie-parser": "^1.4.6",
    "drizzle-orm": "^0.30.9",
    "express": "^4.21.2",
    "express-session": "^1.18.0",
    "jsdom": "^26.0.0",
    "mysql2": "^3.9.7",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "winston": "^3.12.0",
    "winston-transport-sentry-node": "^3.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@clubs/eslint-config": "workspace:*",
    "@clubs/typescript-config": "workspace:*",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.8",
    "dotenv-cli": "^7.4.4",
    "drizzle-kit": "^0.20.17",
    "eslint": "^9.24.0",
    "jest": "30.0.5",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@9.14.4"
}
