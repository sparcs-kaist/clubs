{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "tasks": {
    "dev": {
      "dependsOn": ["@clubs/interface#build", "@clubs/domain#build"],
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "clean": {},
    "web#dev-mock": {
      "dependsOn": ["@clubs/interface#build", "@clubs/domain#build"],
      "persistent": true
    },
    "web#storybook": {
      "persistent": true
    },
    "lint": {}
  }
}
