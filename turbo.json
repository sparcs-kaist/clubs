{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "tasks": {
    "web#dev": {
      "dependsOn": ["@clubs/interface#build", "@clubs/domain#build"],
      "persistent": true
    },
    "api#dev": {
      "dependsOn": ["@clubs/interface#build", "@clubs/domain#build"],
      "persistent": true
    },
    "web#dev-mock": {
      "dependsOn": ["@clubs/interface#build", "@clubs/domain#build"],
      "persistent": true
    },
    "web#storybook": {
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "lint": {}
  }
}
